{
  "name": "printer",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "serve": "vue-cli-service serve",
    "seo:prepare": "node scripts/generate-seo-assets.js",
    "build": "npm run seo:prepare && vue-cli-service build",
    "build:ci": "npm run build && npm run check:bundle-budget",
    "check:bundle-budget": "node scripts/check-bundle-budget.js",
    "lint": "vue-cli-service lint",
    "test:unit": "node scripts/run-vitest.js run",
    "test:unit:watch": "node scripts/run-vitest.js",
    "test:unit:coverage": "node scripts/run-vitest.js run --coverage",
    "test:e2e:install": "node scripts/run-playwright.js install chromium",
    "test:e2e:smoke": "node scripts/run-playwright.js test -c playwright.smoke.config.cjs",
    "test:e2e:smoke:headed": "node scripts/run-playwright.js test -c playwright.smoke.config.cjs --headed",
    "gen:api-types": "node scripts/generate-api-types.js",
    "check:api-types": "npm run gen:api-types && node scripts/check-api-types.js"
  },
  "dependencies": {
    "@tiptap/extension-image": "^3.19.0",
    "@tiptap/extension-link": "^3.19.0",
    "@tiptap/extension-placeholder": "^3.19.0",
    "@tiptap/extension-table": "^3.19.0",
    "@tiptap/extension-table-cell": "^3.19.0",
    "@tiptap/extension-table-header": "^3.19.0",
    "@tiptap/extension-table-row": "^3.19.0",
    "@tiptap/extension-underline": "^3.19.0",
    "@tiptap/starter-kit": "^3.19.0",
    "@tiptap/vue-3": "^3.19.0",
    "@yandex/ymaps3-types": "^0.0.27",
    "axios": "^0.27.2",
    "core-js": "^3.34.0",
    "pug": "^3.0.2",
    "pug-plain-loader": "^1.1.0",
    "swiper": "^8.4.7",
    "three": "^0.181.1",
    "vee-validate": "^4.12.2",
    "vue": "^3.3.10",
    "vue-awesome-swiper": "^5.0.1",
    "vue-imask": "^7.6.1",
    "vue-router": "^4.2.5",
    "vue-tsc": "^2.0.21",
    "vue-yandex-maps": "^2.1.2",
    "vuex": "^4.1.0",
    "yup": "^0.32.11"
  },
  "devDependencies": {
    "@babel/eslint-parser": "^7.28.5",
    "@playwright/test": "1.51.1",
    "@prerenderer/renderer-puppeteer": "^1.2.4",
    "@prerenderer/webpack-plugin": "^5.3.10",
    "@vitejs/plugin-vue": "5.2.4",
    "@vitest/coverage-v8": "0.34.6",
    "@vue/cli-plugin-babel": "^5.0.8",
    "@vue/cli-plugin-eslint": "^5.0.8",
    "@vue/cli-plugin-router": "^5.0.8",
    "@vue/cli-plugin-vuex": "^5.0.8",
    "@vue/cli-service": "^5.0.8",
    "@vue/compiler-sfc": "^3.0.0",
    "@vue/eslint-config-standard": "^8.0.1",
    "@vue/test-utils": "2.4.6",
    "eslint": "^8.57.1",
    "eslint-config-standard": "^17.1.0",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-n": "^15.7.0",
    "eslint-plugin-promise": "^6.6.0",
    "eslint-plugin-vue": "^9.32.0",
    "jsdom": "22.1.0",
    "openapi-typescript": "^7.13.0",
    "sass": "^1.26.5",
    "sass-loader": "^8.0.2",
    "typescript": "^5.9.3",
    "vitest": "0.34.6"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/vue3-essential",
      "@vue/standard"
    ],
    "parserOptions": {
      "parser": "@babel/eslint-parser",
      "requireConfigFile": false
    },
    "rules": {}
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not dead"
  ]
}
